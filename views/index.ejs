setInterval(updateCountdown, 1000)


function updateCountdown() {
    const minutes = Math.floor(time / 60)
    let seconds = time % 60
    // let seconds = seconds <10 ? '0' + seconds : seconds;

    countdownEl.innerHTML = `12:${minutes}:${seconds}`;
    time--;

